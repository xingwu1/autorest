 */
function @(Model.MethodGroupType)(client) {
  this.client = client;
}

@EmptyLine
@foreach (var method in Model.MethodTemplateModels)
{
@:@(Include(new MethodTemplate(), method))
@EmptyLine
}
@EmptyLine
module.exports = @(Model.MethodGroupType);
