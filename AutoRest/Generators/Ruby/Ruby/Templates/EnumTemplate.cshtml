@inherits Microsoft.Rest.Generator.Template<Microsoft.Rest.Generator.Ruby.EnumTemplateModel>
# encoding: utf-8
@Header("# ")
@EmptyLine
module @Settings.Namespace
  module Models
    #
    @WrapComment("# ", "Defines values for " + Model.TypeDefinitionName)
    #
    module @(Model.TypeDefinitionName)
      @for (int i = 0; i < Model.Values.Count; i++)
      {
      @:@(Model.Values[i].Name) = "@(Model.Values[i].SerializedName)"
      }
    end
  end
end
