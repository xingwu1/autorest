<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<package xmlns="http://schemas.microsoft.com/packaging/2011/08/nuspec.xsd">
  <metadata minClientVersion="2.5">
    <id>Microsoft.Rest.ClientRuntime</id>
    <title>Client Runtime Library for Microsoft AutoRest Generated Clients</title>
    <version>$version$</version>
    <authors>Microsoft</authors>
    <owners>azure-sdk, Microsoft</owners>
    <licenseUrl>https://raw.githubusercontent.com/Microsoft/dotnet/master/LICENSE</licenseUrl>
    <projectUrl>https://github.com/Azure/AutoRest</projectUrl>
    <iconUrl>https://cdn.rawgit.com/Azure/AutoRest/7c1576dfb56974176223545cfac5762d168ded5f/Documentation/images/autorest-small-flat.png</iconUrl>
    <requireLicenseAcceptance>true</requireLicenseAcceptance>
    <summary>Provides HttpClient infrastructure for clients generated by AutoRest.</summary>
    <description>Infrastructure for error handling, tracing, and HttpClient pipeline configuration. Required by client libraries generated using AutoRest.

      Supported Platforms:
      -   Portable Class Libraries
      -   .NET Framework 4.5
      -   Windows 8
      -   Windows Phone 8.1
      -   DotNet Core</description>
    <copyright>Copyright (c) Microsoft Corporation</copyright>
    <tags>Microsoft AutoRest ClientRuntime REST</tags>
    <dependencies>
      <group targetFramework="net45">
        <dependency id="Newtonsoft.Json" version="6.0.8"/>
      </group>
      <group targetFramework="portable-net45+win8+wpa81">
        <dependency id="Newtonsoft.Json" version="6.0.8"/>
      </group>
      <group targetFramework="dotnet">
        <dependency id="System.Diagnostics.Tools" version="4.0.0" />
        <dependency id="System.Net.Http" version="4.0.0" />
        <dependency id="System.Threading.Tasks" version="4.0.10" />
        <dependency id="System.Xml.ReaderWriter" version="4.0.10" />
        <dependency id="Microsoft.CSharp" version="4.0.0" />
        <dependency id="Newtonsoft.Json" version="6.0.8"/>
      </group>
    </dependencies>
    <frameworkAssemblies>
      <frameworkAssembly assemblyName="System.Net.Http" targetFramework="net45"/>
    </frameworkAssemblies>
  </metadata>
  <files>
    <file src="ClientRuntimes/CSharp/ClientRuntime/**/*.cs" target="src/Common"/>
    <file src="ClientRuntimes/CSharp/ClientRuntime/Properties/*.resx" target="src/Common/Properties"/>
    <file src="binaries/portable/Microsoft.Rest.ClientRuntime.dll" target="lib/portable-net45+win8+wpa81"/>
    <file src="binaries/portable/Microsoft.Rest.ClientRuntime.pdb" target="lib/portable-net45+win8+wpa81"/>
    <file src="binaries/portable/Microsoft.Rest.ClientRuntime.xml" target="lib/portable-net45+win8+wpa81"/>
    <file src="binaries/portable/Microsoft.Rest.ClientRuntime.dll" target="lib/dotnet"/>
    <file src="binaries/portable/Microsoft.Rest.ClientRuntime.pdb" target="lib/dotnet"/>
    <file src="binaries/portable/Microsoft.Rest.ClientRuntime.xml" target="lib/dotnet"/>
  </files>
</package>
